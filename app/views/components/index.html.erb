<div>
  <table>
    <thead>
    <th>id</th>
    <th>describe</th>
    <th>type</th>
    <th>state</th>
    <th>created_time</th>
    <th>updated_time</th>
    </thead>

    <tbody>
    <% @components.each do |component| %>
    <tr>
      <td><%= component.id %></td>
      <td><%= component.describe %></td>
      <td><%= component.component_type_id %></td>
      <td><%= component.component_state_id %></td>
      <td><%= component.created_at %></td>
      <td><%= component.updated_at %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>